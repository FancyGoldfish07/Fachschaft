<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Priority</th>
      <th>Flag</th>
      <th>Imageurl</th>
      <th>Externallink</th>
      <th>Revision</th>
      <th>Typeofdate</th>
      <th>Editingdeadline</th>
      <th>Usergroup</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.date %></td>
        <td><%= event.priority %></td>
        <td><%= event.flag %></td>
        <td><%= event.imageURL %></td>
        <td><%= event.externalLink %></td>
        <td><%= event.revision %></td>
        <td><%= event.typeOfDate %></td>
        <td><%= event.editingDeadline %></td>
        <td><%= event.userGroup %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>

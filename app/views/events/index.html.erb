<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Start</th>
    <th>Priority</th>
    <th>Flag</th>
    <th>Imageurl</th>
    <th>Url</th>
    <th>Repeat</th>
    <th>Typeofdate</th>
    <th>End</th>
    <th>Usergroup</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start %></td>
        <td><%= event.priority %></td>
        <td><%= event.flag %></td>
        <td><%= event.imageURL %></td>
        <td><%= event.url %></td>
        <td><%= event.repeat %></td>
        <td><%= event.typeOfDate %></td>
        <td><%= event.end %></td>
        <td><%= event.userGroup %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
<div class="panel-body">
  <div id="calendar">

  </div>
</div>